{
  "name": "re-radio-server-next",
  "private": true,
  "scripts": {
    "preinstall": "cp -n .env.example .env",
    "lint": "eslint --ext .ts src/",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts src/",
    "start": "run-p start:**",
    "start:graphql": "ts-node src/radio.ts",
    "dev": "run-p dev:**",
    "dev:graphql": "ts-node-dev --no-deps --respawn --dedupe src/radio.ts",
    "db": "docker-compose -f ./docker-compose.db.yml --project-directory . up",
    "typeorm": "ts-node src/typeorm-cli.ts --config src/typeorm-options.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "worker": "ts-node src/workers.ts",
    "worker:seed": "npm run worker -- seed",
    "worker:seed:reset": "npm run worker -- seed:reset"
  },
  "dependencies": {
    "@nestjs/common": "6.7.1",
    "@nestjs/core": "6.7.1",
    "@nestjs/graphql": "6.5.1",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.7.1",
    "@nestjs/typeorm": "6.2.0",
    "apollo-server-express": "2.9.3",
    "bcrypt-nodejs": "0.0.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.0",
    "dotenv": "8.1.0",
    "get-video-id": "3.1.4",
    "graphql": "14.5.7",
    "moment": "2.24.0",
    "node-fetch": "2.6.0",
    "passport": "0.4.0",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.19",
    "type-graphql": "0.17.5",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/graphql": "14.2.3",
    "@types/node": "10.14.7",
    "@types/node-fetch": "2.5.2",
    "@types/passport-jwt": "3.0.2",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-prettier": "3.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "ts-node": "8.4.1",
    "ts-node-dev": "1.0.0-pre.43",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.6.3",
    "wait-on": "3.3.0"
  },
  "license": "MIT"
}
