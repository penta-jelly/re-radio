{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Radio server",
      "sourceMaps": true,
      "restart": true,
      "protocol": "inspector",
      "port": 2997,
      "outFiles": ["${workspaceFolder}/server/lib/**/*.js"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Real time radio server",
      "sourceMaps": true,
      "restart": true,
      "protocol": "inspector",
      "port": 2998,
      "outFiles": ["${workspaceFolder}/server/lib/**/*.js"]
    },
    {
      "name": "Chrome: Client",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/client/src"
    }
  ]
}
