{
  "name": "re-radio-common",
  "version": "0.1.0-SNAPSHOT",
  "description": "Common package for sharing code between mono repositories",
  "main": "lib/index.js",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint --ext .ts src/ && prettier --check src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts src/",
    "dev": "run-p dev:**",
    "dev:typescript": "tsc --watch --preserveWatchOutput"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.1",
    "prettier": "1.19.1",
    "typescript": "3.7.3"
  }
}
