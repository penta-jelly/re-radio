{
  "name": "re-radio-server",
  "version": "0.1.0-SNAPSHOT",
  "scripts": {
    "initialize": "shx cp -n .env.example .env",
    "compile": "tsc",
    "lint": "eslint --ext .ts src/ && prettier --check src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts src/",
    "prestart": "rm -rf lib",
    "start": "run-p start:**",
    "start:typescript": "tsc --watch --preserveWatchOutput",
    "start:server": "wait-on lib/main.js && nodemon --inspect=2998 lib/main.js serve all",
    "start:seed": "wait-on lib/main.js && node lib/main.js run seed",
    "ci:start": "npm-run-all --parallel ci:start:**",
    "ci:start:server": "node lib/main.js serve all",
    "ci:start:seed": "node lib/main.js run seed",
    "db": "npm run db:up",
    "db:up": "docker-compose -f ./docker-compose.db.yml --project-directory . up",
    "db:down": "docker-compose -f ./docker-compose.db.yml --project-directory . down",
    "typeorm": "node lib/typeorm-cli.js --config lib/typeorm-options.js",
    "typeorm:migrate": "npm run -- typeorm migration:generate -n",
    "worker": "node lib/main.js run",
    "seed": "npm run worker -- seed",
    "seed:song": "npm run worker -- seed:song",
    "seed:reset": "npm run worker -- seed:reset"
  },
  "dependencies": {
    "@nestjs/common": "7.0.8",
    "@nestjs/core": "7.0.8",
    "@nestjs/graphql": "7.3.0",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.8",
    "@nestjs/serve-static": "2.1.0",
    "@nestjs/typeorm": "7.0.0",
    "apollo-server-express": "2.12.0",
    "bcrypt-nodejs": "0.0.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.1",
    "dotenv": "8.2.0",
    "get-video-id": "3.1.4",
    "graphql": "15.0.0",
    "graphql-redis-subscriptions": "2.2.1",
    "graphql-subscriptions": "1.1.0",
    "moment": "2.24.0",
    "nest-winston": "1.3.3",
    "node-fetch": "2.6.0",
    "passport": "0.4.1",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "pg": "8.0.2",
    "re-radio-common": "^0.1.0-SNAPSHOT",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "typeorm": "0.2.24",
    "winston": "3.2.1",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/graphql": "14.2.3",
    "@types/node": "10.14.7",
    "@types/node-fetch": "2.5.6",
    "@types/passport-jwt": "3.0.3",
    "@types/yargs": "15.0.4",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "nodemon": "2.0.3",
    "npm-run-all": "4.1.5",
    "pkg": "4.4.7",
    "prettier": "1.19.1",
    "shx": "0.3.2",
    "source-map-support": "0.5.16",
    "typescript": "3.8.3",
    "wait-on": "4.0.2"    
  },
  "bin": "lib/main.js",
  "pkg": {
    "assets": ["lib/migrations/**/*"]
  },
  "license": "MIT"
}
