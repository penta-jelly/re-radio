{
  "name": "re-radio-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.ts\" --ignore-path src/prisma/prisma.binding.ts",
    "build": "tsc -p .",
    "start": "node dist/main.js",
    "dev": "nodemon -L",
    "prisma": "cd database && prisma",
    "prisma:deploy": "npm run prisma -- deploy -e ../.env",
    "prisma:reset": "npm run prisma -- reset -e ../.env",
    "graphql:get-schema": "graphql get-schema --project database",
    "graphql:codegen": "graphql codegen --project database",
    "worker": "node dist/workers.js",
    "worker:seed": "npm run worker -- seed"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.0.0",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "apollo-server-express": "^2.4.6",
    "bcrypt-nodejs": "0.0.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "prisma-binding": "^2.1.6",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/graphql": "^14.0.7",
    "@types/passport-jwt": "^3.0.1",
    "graphql-cli": "^3.0.11",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "tslint": "^5.13.0",
    "typescript": "^3.3.0"
  },
  "description": "description",
  "author": {
    "name": "Penta Jelly",
    "email": "pvtri96@gmail.com"
  },
  "license": "MIT"
}
