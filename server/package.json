{
  "name": "re-radio-server",
  "private": true,
  "scripts": {
    "preinstall": "cp -n .env.example .env",
    "lint": "eslint --ext .ts src/",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts src/",
    "start": "run-p start:**",
    "start:graphql": "ts-node src/radio.ts",
    "start:real-time": "ts-node src/real-time-radio.ts",
    "dev": "run-p dev:**",
    "dev:graphql": "ts-node-dev --no-deps --respawn --dedupe src/radio.ts",
    "dev:real-time": "ts-node-dev --no-deps --respawn --dedupe src/real-time-radio.ts",
    "prisma": "ts-node src/prisma.ts",
    "graphql:get-schema": "graphql get-schema --project database",
    "graphql:codegen": "graphql codegen --project database",
    "worker": "ts-node src/workers.ts",
    "worker:seed": "npm run worker -- seed"
  },
  "dependencies": {
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "@nestjs/graphql": "6.4.2",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.5.3",
    "apollo-server-express": "2.8.2",
    "bcrypt-nodejs": "0.0.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.0",
    "dotenv": "8.1.0",
    "get-video-id": "3.1.4",
    "graphql": "14.5.0",
    "graphql-cli": "3.0.14",
    "graphql-tools": "4.0.5",
    "moment": "2.24.0",
    "node-fetch": "2.6.0",
    "passport": "0.4.0",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "prisma": "1.34.6",
    "prisma-binding": "2.3.15",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/graphql": "14.2.3",
    "@types/node": "10.14.7",
    "@types/node-fetch": "2.5.0",
    "@types/passport-jwt": "3.0.1",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "eslint": "6.0.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.0",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.0.0-pre.41",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.5.3",
    "wait-on": "3.3.0"
  },
  "license": "MIT"
}
